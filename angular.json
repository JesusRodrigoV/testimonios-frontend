{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "testimonios-frontend": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss",
          "changeDetection": "OnPush"
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/testimonios-frontend",
            "index": "src/index.html",
            "browser": "src/main.ts",
            "polyfills": [],
            "tsConfig": "tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "./node_modules/leaflet/dist/images",
                "output": "leaflet/"
              },
              {
                "glob": "**/*",
                "input": "public/images",
                "output": "/assets/images"
              },
              {
                "glob": "**/*",
                "input": "public/videos",
                "output": "/assets/videos"
              },
              {
                "glob": "**/*",
                "input": "public/audios",
                "output": "/assets/audios"
              },
              {
                "glob": "**/*",
                "input": "public/icons",
                "output": "/assets/icons"
              },
              {
                "glob": "**/*",
                "input": "public/fonts",
                "output": "/assets/fonts"
              },
              {
                "glob": "favicon.ico",
                "input": "public",
                "output": "/"
              },
              {
                "glob": "favicon-96x96.png",
                "input": "public",
                "output": "/"
              },
              {
                "glob": "favicon.svg",
                "input": "public",
                "output": "/"
              },
              {
                "glob": "apple-touch-icon.png",
                "input": "public",
                "output": "/"
              },
              {
                "glob": "site.webmanifest",
                "input": "public",
                "output": "/"
              },
              {
                "glob": "web-app-manifest-*.png",
                "input": "public",
                "output": "/"
              }
            ],
            "styles": [
              "@angular/material/prebuilt-themes/cyan-orange.css",
              "node_modules/swiper/swiper-bundle.min.css",
              "node_modules/boxicons/css/boxicons.min.css",
              "node_modules/@videogular/ngx-videogular/fonts/videogular.css",
              "node_modules/leaflet/dist/leaflet.css",
              "src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kB",
                  "maximumError": "1MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4kB",
                  "maximumError": "10kB"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "testimonios-frontend:build:production"
            },
            "development": {
              "buildTarget": "testimonios-frontend:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": ["zone.js", "zone.js/testing"],
            "tsConfig": "tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "public"
              }
            ],
            "styles": [
              "@angular/material/prebuilt-themes/cyan-orange.css",
              "src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    }
  }
}
